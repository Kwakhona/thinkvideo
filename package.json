    "scripts": {
        "clean-dist": "rimraf dist",
        "copy-files": "mkdir dist && cp app/index.html dist && cp app/favicon.ico dist",
        "clean-and-copy": "npm run clean-dist && npm run copy-files",
        "prestart": "npm run clean-and-copy",
        "start": "env-cmd ./.env webpack-dev-server --config ./build/webpack.config.js --open --inline --history-api-fallback --content-base dist",
        "prebuild": "npm run clean-and-copy",
        "build": "env-cmd ./.env webpack --config build/webpack.config.js",
        "test": "env-cmd .env.local karma start test/karma.ci.conf.js",
        "test:watch": "env-cmd .env.local karma start test/karma.conf.js",
        "lint": "eslint --fix app/**.js app/**/**.js app/**/**/**.js app/**/**/**/**.js app/**/**/**/**/**.js",
        "precommit": "npm test && npm run lint",
        "prebuild:docker": "npm run clean-and-copy",
        "build:docker": "webpack --config build/webpack.config.js"
    }
